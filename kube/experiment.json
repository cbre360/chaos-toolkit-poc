{
  "chaoslib_version": "1.7.1",
  "title": "Microservice first test",
  "description": "First attempt",
  "id": "73bffdf6-8925-4d47-8a38-4492528db75e",
  "target": "kubernetes",
  "date": "2019-11-25T20:42:06.428976Z",
  "secrets": {
    "kubernetes": {
      "KUBERNETES_HOST": "https://aks-1947bd62.hcp.eastus2.azmk8s.io:443",
      "KUBERNETES_API_KEY": {
        "type": "env",
        "key": ""
      }
    }
  },
  "extension": {
    "name": "chaostoolkit-kubernetes",
    "version": "0.22.0"
  },
  "activities": [
    {
      "name": "all-our-microservices-should-be-healthy",
      "type": "probe",
      "tolerance": true,
      "provider": {
        "type": "python",
        "module": "chaosk8s.probes",
        "func": "microservice_available_and_healthy",
        "arguments": {
          "name": "details",
          "ns": "default"
        }
      }
    },
    {
      "type": "action",
      "name": "terminate-db-pod",
      "provider": {
        "type": "python",
        "module": "chaosk8s.pod.actions",
        "func": "terminate_pods",
        "arguments": {
          "label_selector": "app=host-service-audit",
          "name_pattern": "host-service-[0-9]$",
          "rand": true,
          "ns": "default"
        }
      },
      "pauses": {
        "after": 5
      }
    }
  ],
  "system": null
}